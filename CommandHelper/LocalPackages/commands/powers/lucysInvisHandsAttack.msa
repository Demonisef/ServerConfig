invishandsattack:/invishandsattack $vic = >>>

    @vic = $vic
    @vic = player(@vic)
    @plr = player()
    @ploc = ploc(@plr)

    if(ponline(@vic)){
        @plrs = players_in_radius(@ploc,10)
        foreach(@player in @plrs){
            if(@vic == @player){
                damage_entity(puuid(@vic),40,puuid(@plr))
            }else{
                tmsg(@plr,color(red).'You are not close enough to that player!')
            }
        }   
    }

<<<
